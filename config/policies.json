{
  "policies": [
    {
      "name": "api-key-inbound",
      "policyType": "api-key-inbound",
      "handler": {
        "module": "$import(@zuplo/runtime)",
        "export": "ApiKeyInboundPolicy",
        "options": {
          "allowUnauthenticatedRequests": false,
          "bucketName": "zprj-js1d7dq2a3vik1va3cux9dyt-production",
          "cacheTtlSeconds": 60
        }
      }
    },

    {
      "name": "inject-casino-token",
      "policyType": "set-headers-inbound",
      "handler": {
        "export": "SetHeadersInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "headers": [
            {
              "name": "cookie",
              "value": "$env(CASINO_COOKIE)"
            }
          ]
        }
      }
    },

    {
      "name": "sanitize-leaderboard-data",
      "policyType": "custom-code-outbound",
      "handler": {
        "module": "$import(./modules/sanitize-leaderboard-data)",
        "export": "default"
      }
    }
  ]
}
