{
  "openapi": "3.1.0",
  "info": {
    "title": "My Zuplo API",
    "version": "1.0.0"
  },
  "paths": {
    "/leaderboard": {
      "x-zuplo-path": {
        "pathMode": "open-api"
      },
      "get": {
        "summary": "Retrieve sanitized leaderboard data",
        "description": "Sanitizes data",
        "x-zuplo-route": {
          "corsPolicy": "anything-goes",
          "handler": {
            "export": "urlRewriteHandler",
            "module": "$import(@zuplo/runtime)",
            "options": {
              "rewritePattern": "${\"\"}${env.CASINO_URL}"
            }
          },
          "policies": {
            "inbound": [
              "api-key-inbound"
            ],
            "outbound": [
              "sanitize-leaderboard-data"
            ]
          }
        },
        "operationId": "leaderboard"
      }
    }
  }
}